{
  "name": "relay-faffing",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@artsy/palette": "^4.14.40",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.1.0",
    "react-relay": "^5.0.0",
    "react-scripts": "3.0.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "schema": "yarn run fetchV2schema",
    "fetchV2schema": "curl -o schema.graphql https://raw.githubusercontent.com/artsy/metaphysics/master/_schemaV2.graphql"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-relay": "^5.0.0",
    "graphql": "^14.4.2",
    "relay-compiler": "^5.0.0"
  }
}
